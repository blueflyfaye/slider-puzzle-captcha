{
  "name": "slider-puzzle-captcha",
  "version": "0.1.0",
  "repository": {
    "url": "https://github.com/superwf/slider-puzzle-captcha.git",
    "type": "git"
  },
  "description": "drag puzzle to validate captcha",
  "main": "es/client/export.js",
  "less": "src/less/index.less",
  "files": [
    "build",
    "src"
  ],
  "scripts": {
    "start": "[ ! -d public ] && mkdir public;gulp",
    "test": "cross-env BABEL_ENV=commonjs mocha test/server --compilers js:babel-register --recursive",
    "test:watch": "npm test -- --watch",
    "test:client": "cross-env NODE_ENV=test karma start",
    "production": "cross-env NODE_ENV=production pm2 start src/server/index.js -i 2",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir build",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es"
  },
  "keywords": [
    "puzzle",
    "captcha",
    "slider"
  ],
  "author": "superwf",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.15.2",
    "canvas": "^1.4.0",
    "co-body": "^4.2.0",
    "es6-promise": "^3.2.1",
    "express": "^4.14.0",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.0.0",
    "lodash": "^4.13.1",
    "microevent": "^1.0.0",
    "morgan": "^1.7.0",
    "redis": "^2.6.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "babel": "^6.5.2",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-3": "^6.11.0",
    "babel-register": "^6.9.0",
    "babelify": "^7.3.0",
    "browser-sync": "^2.13.0",
    "browserify": "^13.0.1",
    "cross-env": "^1.0.8",
    "eslint": "^3.0.1",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-react": "^5.2.2",
    "exorcist": "^0.4.0",
    "expect": "^1.20.2",
    "gulp": "^3.9.1",
    "gulp-browserify": "^0.5.1",
    "gulp-buffer": "0.0.2",
    "gulp-clean-css": "^2.0.11",
    "gulp-eslint": "^3.0.1",
    "gulp-less": "^3.1.0",
    "gulp-load-plugins": "^1.2.4",
    "gulp-pngmin": "^0.1.4",
    "gulp-postcss": "^6.1.1",
    "gulp-pug": "^3.0.3",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.4",
    "karma": "^1.1.0",
    "karma-browserify": "^5.0.5",
    "karma-chrome-launcher": "^1.0.1",
    "karma-mocha": "^1.1.1",
    "mocha": "^2.5.3",
    "pump": "^1.0.1",
    "sinon": "^1.17.4",
    "supertest": "^1.2.0",
    "uglify-js": "^2.7.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0",
    "webpack": "^1.13.1"
  }
}
